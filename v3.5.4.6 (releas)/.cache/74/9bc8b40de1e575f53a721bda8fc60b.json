\n\r\na:hover{\r\n    color: #ffffff;\r\n}\r\n\r\na:hover:after,\r\na:focus:after {\r\n\twidth: 100%; /*устанавливаем значение 100% чтобы ссылка подчёркивалась полностью*/\r\n}\r\n\r\n/* Page container settings */\r\n.cont{\r\n    width: 98%;\r\n    margin: 0 auto;\r\n}\r\n\r\n.head{\r\n    text-align: center;\r\n    align-items: center;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    /* background-color: #79a0c1; */\r\n}\r\n.head h3{\r\n    color: #ffffff;\r\n}\r\n\r\n.menu{\r\n    width: 400px;\r\n    display: flex;\r\n    justify-content: space-between;\r\n}\r\n\r\n/* Partition style */\r\n.wrapper{\r\n    min-height: 100%;\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\n\r\n.header{\r\n    position: fixed;\r\n    width: 100%;\r\n    align-items: center;\r\n    /* display: flex; */\r\n    box-shadow: 0 0 10px rgba(0,0,0,0.6);\r\n    background-color: rgba(121, 160, 193, 0.97);\r\n    justify-content: space-evenly;\r\n    padding-top: 15px;\r\n    padding-bottom: 15px;\r\n}\r\n\r\n.section{\r\n    flex: 1 1 auto;\r\n}\r\n\r\n.footer{\r\n    background-color: #975a58;\r\n    padding-top: 15px;\r\n    padding-bottom: 15px;\r\n}\r\n\r\n/* Modal style */\r\n.modal-footer{\r\n    padding-bottom: 0;\r\n    justify-content: space-evenly;\r\n}\r\n\r\n.form-control{\r\n    margin-top: 5px;\r\n}\r\n\r\n.btn-close:focus{\r\n    box-shadow: none;\r\n}\r\n\r\n/* Button style */\r\n.EntranceBtn{\r\n    display: inline-block;\r\n    font-weight: 400;\r\n    line-height: 1.5;\r\n    color: #ffffff;\r\n    text-align: center;\r\n    text-decoration: none;\r\n    user-select: none;\r\n    background-color: transparent;\r\n    border: 1px solid transparent;\r\n    font-size: 1rem;\r\n}\r\n\r\n.menu-btn{\r\n    color: #ffffff;\r\n    box-shadow: none;\r\n}\r\n.menu-btn:focus{\r\n    box-shadow: none;\r\n}\r\n\r\n/*Иные стили*/\r\n.error{\r\n    color: #dc3545;\r\n}\r\n\r\n.reg{\r\n    color: #198754;\r\n}\r\n\r\ninput[type=file]{\r\n    display: none;\r\n}\r\n\r\n.preview{\r\n    /* display: flex;\r\n    flex-wrap: wrap; */\r\n    padding: .5rem;\r\n    padding-left: 0px;\r\n}\r\n\r\n.preview-file {\r\n    position: relative;\r\n    white-space: nowrap;\r\n    margin-bottom: .5rem;\r\n    margin-right: .5rem;\r\n    overflow: hidden;\r\n    /* width: 150px;\r\n    height: 70px;\r\n    text-align: justify; */\r\n}\r\n.preview-file.removing{\r\n    transform: scale(0);\r\n    transition: .3s;\r\n}\r\n.preview-file:hover .preview-remove{\r\n    opacity: 1;\r\n}\r\n.preview-remove{\r\n    opacity: 0;\r\n    width: 20px;\r\n    height: 20px;\r\n    position: absolute;\r\n    right: 0;\r\n    top: 0;\r\n    font-weight: bold;\r\n    background: rgba(255, 255, 255, .5);\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    cursor: pointer;\r\n    transition: opacity .22s;\r\n}\r\n/* .preview-info{\r\n    height: 20px;\r\n} */\r\n.preview-info-progress{\r\n    height: 5px;\r\n    left: 0;\r\n    top: 0;\r\n    bottom: 0;\r\n    background: #198754;\r\n    text-align: center;\r\n    transition: width .22s;\r\n}"},"lineCount":192}},"error":null,"hash":"2c60c5a329d5f8e0a836249a0794fcf9","cacheData":{"env":{}}}